{
    "name": "jamierumbelow/crudcontroller",
    "description": "An extensible, simple base CRUD controller for Laravel 5 applications",
    "keywords": ["crud", "controller", "base", "laravel", "orm"],
    "type": "library",
    "homepage" : "https://github.com/jamierumbelow/crudcontroller",
    "require": {
        "php" : ">=5.4.0",
        "laravel/framework": ">=5.1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Rumbelow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rumbelow\\CrudController\\Test\\": "test"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Rumbelow",
            "email": "jamie@jamierumbelow.net"
        }
    ],
    "scripts": {
        "test": "phpunit --coverage-text"
    },
    "minimum-stability": "dev"
}
